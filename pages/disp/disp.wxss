.overall {
    display: flex;
    margin: 5%;
    padding: 0%;
    width: 100%;
    background-color: whitesmoke;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    --td-cell-height:84rpx;
    --td-cell-bg-color:white;
    --td-cell-title-font-size:32rpx;

}
.title
{
    font-size: large;
    font-weight: bold;
    text-align: left;
    background-color: transparent;
    width: 100%;
}
.overallcircle {
    display: flex;
    flex-direction: column;
    width: 350rpx;
    background-color: transparent;
    margin: 3%;
